{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "author": "ArseniyXaoc",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "commonjs",
  "description": "REST service for RS School",
  "main": "src/server.ts",
  "scripts": {
    "nest:start": "nest start",
    "nest:dev": "nest start --watch",
    "nest:debug": "nest start --debug --watch",
    "start": "nodemon -L src/server.ts",
    "lint": "eslint --ext .js,.ts --fix src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "jsdoc": "./node_modules/.bin/jsdoc src/resources/boards -r -c conf.json",
    "tsstart": "./node_modules/.bin/ts-node src/server",
    "compile": "./node_modules/.bin/tsc && node --experimental-modules build/server.js",
    "dev": "nodemon --exitcrash -L --watch src -e ts --exec \"npm run compile\"",
    "migration": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0 <15"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/typeorm": "^7.1.5",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "7.0.3",
    "dotenv": "9.0.0",
    "express": "4.17.1",
    "joi": "^17.4.0",
    "jsdoc": "^3.6.7",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sequelize": "^6.6.2",
    "swagger-ui-express": "4.1.6",
    "typeorm": "^0.2.34",
    "uuid": "8.3.2",
    "winston": "^3.3.3",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "chai": "4.3.4",
    "debug": "4.3.1",
    "dirty-chai": "2.0.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "tslib": "^2.2.0",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
